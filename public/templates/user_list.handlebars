<div id="content">
    <ul class="users">
    {{#each models}}
    <li data-userName="{{userName}}">
        <img src="/edc/userimage/{{userName}}?size=icon" class="profile"/>
        <a class="fullname">{{fullName}}</a>
        {{#if admin}}
            <span class="administrator">{{t "users.administrator"}}</span>
        {{/if}}
    </li>
    {{/each}}
    </ul>
</div>