<div class="sub_header">
    <span class="canonical_name">{{canonicalName}}</span>
</div>
<ul class="list">
    {{#each models}}
        <li>
            <span class="spacer left"/>

            <img class="image" src="{{iconUrl}}"/>

            <div class="text">
                <div class="main">
                    <span class="name">{{objectName}}</span>
                </div>
                <div class="details">
                    <span class="column_count">{{pluralize columns "dataset.manage_join_tables.column_count" count=columns}}
                </div>
            </div>

            <span class="spacer right"/>
        </li>
    {{/each}}
</ul>
