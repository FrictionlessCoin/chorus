{{> errorDiv }}
<form action="/edc/workspace/" class="new_workspace">
    <input type="text" name="name" placeholder="Name this workspace" tabindex=1></input>
    <input type="checkbox" name="isPublic" id="isPublic" tabindex=2></input><label for="isPublic">Make this workspace available publicly</label>

    <div class="controls">
        <button type="button" class="cancel" tabindex=4>Cancel</button>
        <button type="submit" class="submit" tabindex=3>Create Workspace</button>
    </div>
</form>

