<div class="definition">
    <button class="transform">{{t "dataset.content_details.transform"}}<span class="down_arrow"/></button>
    <div class="transform_options hidden">
        <ul class="transform_menu">
            <li><a href="#" class="visualize">{{t "dataset.content_details.visualize"}}</a></li>
            <li><a href="#" class="derive">{{t "dataset.content_details.derive"}}</a></li>
        </ul>
    </div>
    
</div>
<div class="create_chart hidden">
    <a href="#" class="cancel">{{t "actions.cancel"}}</a>
    <span class="create">{{t "dataset.content_details.create"}}</span><span class="triangle_right"></span>
    <span data-chart_type="frequency" class="chart_icon frequency"><span class="triangle"></span></span>
    <span class="separator"></span>
    <span data-chart_type="histogram" class="chart_icon histogram"><span class="triangle"></span></span>
    <span class="separator"></span>
    <span data-chart_type="heatmap" class="chart_icon heatmap"><span class="triangle"></span></span>
    <span class="separator"></span>
    <span data-chart_type="timeseries" class="chart_icon timeseries"><span class="triangle"></span></span>
    <span class="separator"></span>
    <span data-chart_type="boxplot" class="chart_icon boxplot"><span class="triangle"></span></span>
    <span class="separator"></span>
    <span class="frequency title hidden">{{t "dataset.visualization.names.frequency"}}</span>
    <span class="histogram title hidden">{{t "dataset.visualization.names.histogram"}}</span>
    <span class="heatmap title hidden">{{t "dataset.visualization.names.heatmap"}}</span>
    <span class="timeseries title hidden">{{t "dataset.visualization.names.timeseries"}}</span>
    <span class="boxplot title hidden">{{t "dataset.visualization.names.boxplot"}}</span>
</div>
<div class="create_chorus_view hidden">
     <a href="#" class="cancel">{{t "actions.cancel"}}</a>
      <span class="create">{{t "dataset.content_details.create"}}</span><span class="triangle_right"></span>
     <span class="chorusview"><span class="triangle"></span></span>
</div>
<div class="info_bar hidden">
{{t "dataset.visualization.info_bar"}}
</div>
<div class="column_count">
    <span class="count">{{t "dataset.column_count" count=models.length}}</span>
    <div class="right">
        <a href="#" class="preview">{{t "dataset.data_preview"}}</a>
    </div>
</div>
<div class="chorus_view_info hidden">
{{t "dataset.content_details.derive_info"}}
</div>

<div class="sql_errors hidden">
    <ul>
        <li>{{t "results_console_view.there_was_an_error"}}</li>
    </ul>
    <div class="actions">
        <a href="#" class="action view_error_details">{{t "actions.view_details"}}</a> | <a href="#" class="action close_errors">{{t "actions.close"}}</a>
    </div>
</div>

<div class="data_preview hidden"/>
<div class="filters hidden"/>
