<div class="definition">
    <button class="transform">{{t "dataset.content_details.transform"}}<span class="down_arrow"/></button>
    <div class="transform_options hidden">
        <ul class="transform_menu">
            <li><a href="#" class="visualize">{{t "dataset.content_details.visualize"}}</a></li>
        </ul>
    </div>
    
</div>
<div class="create_chart hidden">
    <a href="#" class="cancel">{{t "actions.cancel"}}</a>
    <span class="create">{{t "dataset.content_details.create"}}</span><span class="triangle_right"></span>
    <span class="chart_icon frequency"></span>
    <span class="chart_icon histogram"></span>
    <span class="chart_icon heatmap"></span>
    <span class="chart_icon timeseries"></span>
    <span class="chart_icon boxplot"></span>
</div>
<div class="column_count">
    <span class="count">{{t "dataset.column_count" count=models.length}}</span>
    <div class="right">
        <a href="#" class="preview">{{t "dataset.data_preview"}}</a>
    </div>
</div>
<div class="data_preview hidden"/>
