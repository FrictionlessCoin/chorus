<h1 class="filter_title">{{t "dataset.filter.title"}}</h1>
<a href="#" class="add_filter">{{t "dataset.filter.add_filter"}}</a>
<ul class="filters">
    <li class="filter">
        <select class="column_filter">
            {{#each models}}
                <option value="{{name}}">{{name}}</option>
            {{/each}}
        </select>
        <span class="remove"></span>
    </li>
</ul>