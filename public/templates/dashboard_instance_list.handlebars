{{#each models}}
    <li data-id="{{id}}">
        <span class="spacer left"/>
        <a class="image">
            <img src="{{imageUrl}}"/>
        </a>
        <div class="text">
            <div class="main">
                <span class="name" title="{{name}}">{{name}}</span>
            </div>
            <div class="browse">
                <a href="#" class="dialog" data-dialog="BrowseDatasets">{{t "dashboard.instances.browse_datasets"}}</a>
            </div>

       </div>
        <span class="spacer right"/>
    </li>
{{/each}}
