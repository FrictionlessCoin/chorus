<div class="brackets">
    <div class="brackets_top"></div>
    <div class="brackets_bottom"></div>
</div>
<select class="column_filter">
    {{#each models}}
        <option value="{{name}}" data-type-category="{{typeCategory}}" {{#if disable}}disabled="disabled"{{/if}}>{{name}}</option>
    {{/each}}
</select>
<select class="comparator"/>
<input class="filter_input validatable hidden" type="text"/>
<input class="time_input validatable hidden" type="text" placeholder="{{{t "dataset.filter.time_placeholder"}}}"/>
<input class="date_input validatable hidden" type="text" placeholder="{{{t "dataset.filter.date_placeholder"}}}"/>
<a href="#" class="remove"><img src="/images/remove_icon.png"/></a>
