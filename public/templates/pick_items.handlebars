{{> errorDiv }}
<div class="sub_header">
    <input type="text" placeholder="{{t placeholderTranslation}}"/>
</div>
{{#if pagination}}
    <div class="pagination content_details"></div>
{{/if}}
{{#if models.length}}
    <div class="items collection_list">
        <ul>
         {{#each models}}
            <li data-id="{{id}}"><img src="{{imageUrl}}"/><span class="name" title="{{name}}">{{name}}</span></li>
         {{/each}}
        </ul>
    </div>
{{else}}
    <div class="none">{{t emptyListTranslationKey}}</div>
{{/if}}

<div class="modal_controls">
    <button type="button" class="cancel" tabindex="3">{{t "actions.cancel"}}</button>
    <button type="submit" class="submit" disabled="disabled" tabindex="2">{{t submitButtonTranslationKey}}</button>
</div>
