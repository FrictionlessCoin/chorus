<div class="spacer_left"></div>
<img class="icon" src="{{iconUrl}}"></img>
<div class="description">
    {{#if displayableFiletype}}
        <a class="name" href="{{showUrl}}">{{displaySearchMatch "name"}}</a>
    {{else}}
        <span class="name">{{displaySearchMatch "name"}}</span>
    {{/if}}
    <div class="location">
        {{t "hdfs.from"}}
        <span class="instance">{{instanceLink}} /</span>
        <span class="path_parts">{{completePath}}</span>
    </div>

    <div class="comments_container"/>
</div>
<div class="spacer_right"></div>
