{{#if shown}}
<div class="details">
    <div class="right">
        {{#if moreResults}}
            <span class="count">{{t "search.count" shown=shown total=total}}</span>
            <a class="show_all" data-type="hdfs" href="#">{{t "search.show_all"}}</a>
        {{else}}
            <span class="count">{{t "search.count_short" shown=shown}}</span>
        {{/if}}
    </div>
    <span class="title">{{t "instances.title"}}</span>
</div>

<ul>
    {{#each models}}
        <li class="result_item" data-id={{id}}>
            <div class="spacer left"></div>
            <div class="images">
                <img class="state" src="{{stateUrl}}" title="{{stateText}}"/>
                <img class="provider" src="{{iconUrl}}"/>
            </div>
            <div class="description"><span class="name">{{{displaySearchMatch "name"}}}</span></div>
            <div class="spacer right"></div>
        </li>
    {{/each}}
</ul>
{{/if}}
