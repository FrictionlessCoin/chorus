{{#if shown}}
    {{#if filteredSearch}}
        <div class="pagination">
            <div class="right">
                <a href="#" class="next">{{t "search.next"}}</a>
                |
                <a href="#" class="previous">{{t "search.previous"}}</a>

            </div>
            <span class="count">{{t "search.results" count=total}}</span>
        </div>
{{else}}
        <div class="details">
            <div class="right">
                {{#if moreResults}}
                    <span class="count">{{t "search.count" shown=shown total=total}}</span>
                    <a class="show_all" data-type="hdfs" href="#">{{t "search.show_all"}}</a>
                {{else}}
                    <span class="count">{{t "search.count_short" shown=shown}}</span>
                {{/if}}
            </div>
            <span class="title">{{t "instances.title"}}</span>
        </div>
{{/if}}
<ul>
    {{#each models}}
        <li class="result_item" data-cid={{model.cid}}>
            <div class="spacer_left"></div>
            <div class="images">
                <img class="state" src="{{stateUrl}}" title="{{stateText}}"/>
                <img class="provider" src="{{iconUrl}}"/>
            </div>
            <div class="description">
                <span class="name">{{{displaySearchMatch "name"}}}</span>
                <div class="comments_container"/>
            </div>
            <div class="spacer_right"></div>
        </li>
    {{/each}}
</ul>
{{/if}}
