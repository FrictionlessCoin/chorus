<input class="search" />
<div class="schema_name">{{t "schema.functions.search_in" schemaName=schemaName}}</div>
{{#if models.length}}
    <ul class="functions list">
        {{#each models}}
            <li title="{{functionName}}">{{functionName}}
                <div class="insert_hover hidden">
                    <div class="arrow_left"/><div class="arrow_left_filling"/>
                    <a class="insert_link" href="#">{{t "schema.functions.insert"}}</a>
                </div>
            </li>
        {{/each}}
    </ul>
{{else}}
    <div class="none_found">{{t "schema.functions.none_found"}}</div>
{{/if}}
