<div class="sub_header">
    <div class="search"></div>
    <div class="sort_menu"/>
</div>
<div class="collection_list">
    <ul>
    {{#each models}}
        <li title="{{fileName}}" data-id="{{id}}">
            <a href="#">
                <img src="{{fileIconUrl fileType "medium"}}"/>
                <span class="name">{{fileName}}</span>
            </a>
        </li>
    {{/each}}
    </ul>
</div>
<div class="modal_controls">
    <button type="button" class="cancel" tabindex=3>{{t "actions.cancel"}}</button>
    <button type="submit" class="submit" disabled="disabled"    tabindex=2>{{t "workfiles.button.attach_file"}}</button>
</div>
