<div class="sub_header">
    <div class="search"></div>
    <div class="sort_menu"/>
</div>
<div class="collection_list">
    <ul>
    {{#each models}}
        <li title="{{fileName}}" data-id="{{id}}">
            <img class="profile" src="{{fileIconUrl fileType "medium"}}"/>
            <span class="name">{{fileName}}</span>
        </li>
    {{/each}}
    </ul>
</div>
<div class="controls">
    <button type="button" class="cancel" tabindex=3>{{t "actions.cancel"}}</button>
    <button type="submit" class="submit" tabindex=2>{{t "workfiles.button.attach_file"}}</button>
</div>
