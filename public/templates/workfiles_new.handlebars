<form action="/edc/workspace/{{workspaceId}}/workfile">
    <input type="text" name="name" placeholder="Name" tabindex="1" maxlength="256"></input>

    <div class="controls">
        <button type="button" class="cancel" tabindex=4>{{t "actions.cancel"}}</button>
        <button type="submit" class="submit" tabindex=3>{{t "workfiles.button.create"}}</button>
    </div>
</form>

