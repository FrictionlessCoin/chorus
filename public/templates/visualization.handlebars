<div class="headerbar">
    <div class="text type">{{t "visualization.type"}}</div>

    <div class="icon boxplot"/>

    <div class="icon chorusview"/>

    <div class="icon frequency"/>

    <div class="icon on heatmap"/>

    <div class="icon histogram"/>

    <div class="icon timeseries"/>

    <div class="text label">Boxplot</div>

    <div class="right">
        <div class="text type">{{t "visualization.category_limit"}}</div>
        <select>
            <option value="1">1</option>
        </select>
        <a href="#" class="filter">{{t "visualization.show_filters" count=filterCount}}</a>
    </div>
</div>

<div class="chart_area">

</div>

<div class="tabledata">

</div>

<div class="controls">
    <div class="vertical">
        <a href="#">{{t "visualization.show_table"}}</a>
        <div class="right">
            <button>{{t "actions.save_as"}}</button>
            <button>{{t "actions.close"}}</button>
        </div>
    </div>
</div>