{{#if shown}}
    <div class="details">
        <div class="right">
            {{#if moreResults}}
                    <span class="count">{{t "search.count" shown=shown total=total}}</span>
                    <a class="show_all" href="#">{{t "search.show_all"}}</a>
                {{else}}
                    <span class="count">{{t "search.count_short" shown=shown}}</span>
                {{/if}}
        </div>
        <span class="title">{{t "search.users.title"}}</span>
    </div>
<ul>
{{#each models}}
<li class="result_item" data-id={{id}}>
    <div class="spacer left"></div>
    <img class="icon" src="{{iconSrc}}"/>
    <div class="description">
        <a class="name" href="{{link}}">{{{displayName}}}</a>
        <div class="supportingMessage">
            {{#each supportingMessage}}
                {{#if title}}
                <div class="title">
                        <span class="prefix">{{t "users.title"}}:</span>
                        <span class="content">&#8220;{{{title}}}&#8221;</span>
                </div>
                {{/if}}
                {{#if ou}}
                <div class="ou">
                        <span class="prefix">{{t "users.department"}}:</span>
                        <span class="content">&#8220;{{{ou}}}&#8221;</span>
                </div>
                {{/if}}
                {{#if content}}
                <div class="notes">
                        <span class="prefix">{{t "users.notes"}}:</span>
                        <span class="content">&#8220;{{{content}}}&#8221;</span>
                </div>
                {{/if}}
                {{#if emailAddress}}
                <div class="email">
                    <span class="content">&#8220;{{{emailAddress}}&#8221;</span>
                </div>
                {{/if}}
                {{#if name}}
                <div class="username">
                        <span class="content">&#8220;{{{name}}&#8221;</span>
                </div>
                {{/if}}
            {{/each}}
        </div>

        {{#if hasMoreSupportingMessage}}
            <a href="#" class="showMoreSupportingMessage"><span class="and">{{t "search.comments_more.and"}}</span>{{t "search.comments_more" count=hasMoreSupportingMessage}}</a>
        {{/if}}

        <div class="moreSupportingMessage hidden">
            {{#each moreSupportingMessage}}
                {{#if title}}
                <div class="title">
                    <span class="prefix">{{t "users.title"}}:</span>
                    <span class="content">&#8220;{{{title}}}&#8221;</span>
                </div>
                {{/if}}
                {{#if ou}}
                <div class="ou">
                    <span class="prefix">{{t "users.department"}}:</span>
                    <span class="content">&#8220;{{{ou}}}&#8221;</span>
                </div>
                {{/if}}
                {{#if content}}
                <div class="notes">
                    <span class="prefix">{{t "users.notes"}}:</span>
                    <span class="content">&#8220;{{{content}}}&#8221;</span>
                </div>
                {{/if}}
                {{#if emailAddress}}
                <div class="email">
                    <span class="content">&#8220;{{{emailAddress}}&#8221;</span>
                </div>
                {{/if}}
                {{#if name}}
                <div class="username">
                    <span class="content">&#8220;{{{name}}&#8221;</span>
                </div>
                {{/if}}
            {{/each}}
        <a href="#" class="hideMoreSupportingMessage">{{t "search.comments_less"}}</a>
        </div>
        <div class="spacer right"></div>
    </div>
</li>
{{/each}}
</ul>

{{/if}}
