{{#if shown}}
    <div class="details">
        <div class="right">
            {{#if moreResults}}
                <span class="count">{{t "search.count" shown=shown total=total}}</span>
                <a class="show_all" href="#">{{t "search.show_all"}}</a>
            {{else}}
                <span class="count">{{t "search.count_short" shown=shown}}</span>
            {{/if}}
        </div>
        <span class="title">{{t "workfiles.title"}}</span>
    </div>

    <ul>
        {{#each models}}
            <li>
                <img class="icon" src="{{iconUrl}}"></img>
                <div class="description">
                    <a class="name" href="{{showUrl}}">{{{name}}}</a>
                    <div class="found_in_workspace">{{{t "search.found_in_workspace" workspaceLink=workspaceLink}}}</div>
                </div>
            </li>
        {{/each}}
    </ul>
{{/if}}
