{{#each models}}
    <li data-id="{{id}}">
        <img src="/edc/workspace/{{id}}/image?size=icon"/>
        <a class="name" href="#/workspaces/{{id}}">{{name}}</a>
        {{#if isPublic}}
            <span class="public tag">{{t "workspaces.public"}}</span>
        {{/if}}
        <a href="#/workspaces/{{id}}/workfiles">(Temporary link to workfiles for workspace)</a>
    </li>
{{/each}}
