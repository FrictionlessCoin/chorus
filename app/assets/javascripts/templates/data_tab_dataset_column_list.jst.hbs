{{#if models.length}}
    <ul class="list">
        {{#each models}}
            <li class='sidebar_list_item' data-name="{{name}}" data-fullname="{{fullName}}" data-cid="{{cid}}" title="{{name}}">
                <span class="spacer_left"/>
                <img src="images/small_column.png">
                <span class="column_type">[{{type}}]</span>
                <span class="column_name">{{name}}</span>
                <span class="spacer_right"/>
            </li>
        {{/each}}
    </ul>
{{else}}
    <div class="none_found">{{t "schema.column.list.empty"}}</div>
{{/if}}
