<form action="/workspaces/{{workspace.id}}/workfiles" enctype="multipart/form-data" method="post">
    {{ uploadWidgetFor "workfile[versions_attributes][0][contents]" }}
    <div class="comment hidden">
        <input type="text" name="workfile[description]" placeholder='{{t "workfiles.import_placeholder"}}' tabindex="1" maxlength="256"></input>
    </div>
    <div class="work_flow_detail hidden">
        {{infoBlock "work_flows.new_dialog.info"}}
        <div class="location_picker"></div>
    </div>
    {{formControlsWithDisabledSubmit "workfiles.button.import"}}
</form>

