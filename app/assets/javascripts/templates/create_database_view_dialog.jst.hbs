<form action="#">
    <div class="target_location">
        {{t "create_database_view.target"}}
        <span class="target_location_value" title="{{canonicalName}}">{{canonicalName}}</span>
    </div>

    <input id="create_database_view_name" placeholder="{{t "create_database_view.name"}}" type="text" name="name" tabindex="1" maxlength="64"/>

    <div class="form_controls">
        <button type="button" class="cancel" tabindex="100">{{t "actions.cancel"}}</button>
        <button type="submit" class="submit" tabindex="99">{{t "create_database_view.submit"}}</button>
    </div>
</form>