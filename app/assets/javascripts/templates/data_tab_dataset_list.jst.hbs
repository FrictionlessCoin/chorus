{{#if models.length}}
    <ul class="list">
        {{#each models}}
            <li data-name="{{name}}" data-fullname="{{fullName}}" data-type="{{type}}" data-cid="{{cid}}" title="{{name}}">
                <span class="spacer_left"/>
                <img src="{{iconUrl}}"/>
                <div class="name">
                    <a href="#" title="{{name}}">{{name}}</a>
                </div>
                <span class="spacer_right"/>
            </li>
        {{/each}}
    </ul>
    {{#if showMoreLink}}
        <a href="#" class="more">{{t "schema.metadata.more"}}</a>
    {{/if}}
{{else}}
    <div class="none_found">{{t "schema.metadata.list.empty"}}</div>
{{/if}}
