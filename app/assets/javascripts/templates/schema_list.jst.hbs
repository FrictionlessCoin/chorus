{{#each models}}
    <li class="schema">
        <span class="spacer_left"/>
        <img src="/images/data_sources/greenplum_schema.png"/>
        <div class="main_text">
            <a class="name" href="{{showUrl}}">{{name}}</a>
            <div class="description">
                {{ datasetMessage }}
            </div>
        </div>
        <span class="spacer_right"/>
    </li>
{{/each}}
