<form action="/workspace/" class="new_workspace">
    <input type="text" name="name" placeholder="Name this workspace" tabindex="1" maxlength="256"></input>
    <input type="checkbox" name="isPublic" id="isPublic" checked="checked" tabindex=2></input><label for="isPublic">{{t "workspaces.make_public"}}</label>

    <div class="modal_controls">
        <button type="button" class="cancel" tabindex=4>{{t "actions.cancel"}}</button>
        <button type="submit" class="submit" disabled="disabled" tabindex=3>{{t "workspaces.button.create"}}</button>
    </div>
</form>

